
@{
    ViewData["Title"] = "saveInDataBase";
}

<h1>saveInDataBase</h1>


<section id="configuration">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-header">
                    <div class="card-title-wrap bar-success">
                        <h4 class="card-title text-right">تقرير حضور وانصراف الموظفين </h4>
                    </div>
                </div>
                <div class="card-body collapse show">
                    
                    <div class="card-block card-dashboard">
                       
                                   
                                 <form asp-controller="EmployeeAttendance" asp-action="saveInDataBase" method="post" enctype="multipart/form-data">
                        
                                     <div class="col-lg-12 d-flex" style="flex-direction:row-reverse">
                                         <input type="file" class="btn btn-success "  id="hidden" name="formFile" style="margin-right: 15px;width: 240px;" />
                                   
                                                <button type="submit" class="btn btn-success ">Add</button>
                                    
                                          
                                     </div>

                                </form>
                                <h1 style="color:red"> @ViewBag.EmployeeProblem</h1>
                                <h1>@ViewBag.EmployeeSuccses</h1>

                  
                         
                         </div>
                 </div>
              
            </div>
        </div>
    </div>
